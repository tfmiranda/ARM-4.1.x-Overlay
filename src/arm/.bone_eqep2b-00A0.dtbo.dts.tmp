/ {
 compatible = "ti,beaglebone", "ti,beaglebone-black";


 part-number = "bone_eqep2b";
 version = "00A0";


 exclusive-use =
  "P8.12",
  "P8.11",
  "P8.16",
  "P8.15",

  "eqep2";

 fragment@0 {
  target = <&am33xx_pinmux>;
  __overlay__ {
   pinctrl_eqep2: pinctrl_eqep2_pins {
    pinctrl-single,pins = <
     0x030 (((1 << 5) | (1 << 3)) | 4)
     0x034 (((1 << 5) | (1 << 3)) | 4)
     0x038 (((1 << 5) | (1 << 3)) | 4)
     0x03C (((1 << 5) | (1 << 3)) | 4)

    >;
   };
  };
 };

 fragment@1 {
  target = <&epwmss2>;
  __overlay__ {
   status = "okay";
  };
 };

 fragment@2 {
  target = <&eqep2>;
  __overlay__ {
   pinctrl-names = "default";
   pinctrl-0 = <&pinctrl_eqep2>;

   count_mode = <1>;





   swap_inputs = <0>;
   invert_qa = <0>;
   invert_qb = <0>;
   invert_qi = <0>;
   invert_qs = <0>;
   omit_interrupt = <0>;

   status = "okay";
  };
 };
};
